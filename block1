#import <Foundation/Foundation.h>

//assign Blocks to printJames variable
void(^printJames)(void) =^(void){
    NSLog(@"Demo Blocks");
};

int main(int argc, const char * argv[]) {
    @autoreleasepool {
        
        //call Blocks
        printJames();
    }
    return 0;
}
