#import <Foundation/Foundation.h>
#import "category_a.h"

@implementation NSString (NSStringCategory )
-(void) ShowJamesName{
    NSLog(@"this is call james Names");
}
@end

int main(int argc, const char * argv[]) {
    @autoreleasepool {
        NSString *ptr = [[NSString alloc ] init ];
        [ptr ShowJamesName];
    }
    return 0;
}
